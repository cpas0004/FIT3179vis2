{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
        "text": "NSW Buildings Destroyed in 2019/20 Bushfires",
        "fontSize": 24
    },
    "width": 600,
    "height": 450,
    "data": {
        "url": "https://raw.githubusercontent.com/cpas0004/FIT3179vis2/main/data/buildings.csv",
        "format": {"type": "csv"}
    },
    "layer": [
        {
            "mark": "bar",
            "encoding": {
                "x": {"field": "Category"},
                "y": {"field": "Value", "type": "quantitative"},
                "xOffset": {"field": "Group"},
                "color": {"field": "Group"}
            }
        }
        
    ]
}